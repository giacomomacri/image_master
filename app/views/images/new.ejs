<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.images() %>">images</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">New image</a></li>
</ul>

<div class="page-header">
    <h1>New image</h1>
</div>

<% var form = formFor(image, {action: pathTo.images(), method: 'POST', id: "image-form", class: 'form-horizontal'}) %>
<%- form.begin() %>
<%- include _form %>
<div class="form-actions">
    <%- form.submit('<i class="icon-ok icon-white"></i> Create image', {class: 'btn btn-primary'}) %> or
    <%- linkTo('Cancel', pathTo.images(), {class: 'btn'}) %>
</div>
<%- form.end() %>
