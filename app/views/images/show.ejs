<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.images() %>">images</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of image</a></li>
</ul>

<div class="page-header">
    <h1>Details of image</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>title</th><td><%= image.title %></td></tr>

        <tr><th>photo</th><td><%= image.photo %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_image(image), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.image(image), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.images()) %>
</div>
